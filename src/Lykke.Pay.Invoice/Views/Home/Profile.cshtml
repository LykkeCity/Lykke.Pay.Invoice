@model Lykke.Pay.Invoice.Models.InvoiceRequest
@{
    ViewData["Title"] = "Profile";
}
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using (Html.BeginForm())
{
    <div class="create">
        <ul class="create__top">
            <li class="create__top-item">
                <a href="#" class="create__top-link">Profile</a>
            </li>
            <li class="create__top-arrow">
                <i class="icon icon--chevron-thin-right"></i>
            </li>
            <li class="create__top-item"> Creat new invoice </li>
        </ul>
        <div class="create__body">
            <div class="create__title">Invoice</div>
            <div class="create__form">
                <div class="create__input-group">
                    <label class="create__input-label">Invoice number</label>
                    <input type="text" asp-for="InvoiceNumber" placeholder="" class="form-control">
                </div>
                <div class="create__input-group">
                    <label class="create__input-label">Client</label>
                    <input type="text" asp-for="Client" placeholder="" class="form-control">
                </div>
                <div class="create__input-group">
                    <label class="create__input-label">Email</label>
                    <input type="email" asp-for="ClientEmail" placeholder="" class="form-control">
                </div>
                <div class="create__input-group">
                    <div class="create__input-amount">
                        <label class="create__input-label">Amount</label>
                        <div class="input-group">
                            <span class="">CHF</span>
                            <input type="text" asp-for="Amount" placeholder="0.00" class="form-control">
                        </div>
                    </div>
                    <div class="create__input-date">
                        <label class="create__input-label">Pay due date</label>
                        <div class="input-group">
                            <i class="icon icon--cal"></i>
                            <input type="text" asp-for="DueDate" placeholder="03.12.2017" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
            <div class="create__item">
                <div class="create__item-name">Status</div>
                <div class="create__item-description">Not created</div>
            </div>
            <div class="create__actions">
                <button type="submit" name="button" class="btn create__button">Generate</button>
                <button type="submit" name="button" class="btn btn--stroke create__link">Safe draft</button>
                <button type="button" name="button" class="btn btn--stroke create__link">Cancel and close</button>
            </div>
        </div>
    </div>
}
